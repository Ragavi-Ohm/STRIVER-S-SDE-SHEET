int AlternatingaMaxLength(vector<int>&nums){
		    // Code here
		    int cnt=0;
		    int n=nums.size();
            int inc=1,dec=1;
            for(int i=1;i<n;i++)
            {
                if(nums[i]>nums[i-1])
                inc=dec+1;
                else if(nums[i]<nums[i-1])
                dec=inc+1;
            }
            return max(inc,dec);
		}
